#import <substrate.h>
#import <rd_route.h> // Biblioteca para redirecionamento de fun√ß√µes

// --- CONFIGURA√á√ïES DE CORES (DESIGN FLIX_FF) ---
#define PURPLE_COLOR [UIColor colorWithRed:0.54 green:0.17 blue:0.89 alpha:1.0]
#define BLACK_COLOR [UIColor colorWithRed:0.0 green:0.0 blue:0.0 alpha:0.95]

// --- VARI√ÅVEIS DE ESTADO ---
static bool isAuthorized = false;
static bool aimbot90 = false;
static bool recoil90 = false;
static bool antenaStealth = false;

// --- FUN√á√ÉO DE TRAVA DE MIRA (AIMBOT 90%) ---
// Hook no motor de rota√ß√£o da Unity
void (*old_SetRotation)(void *instance, struct CGPoint rotation);
void new_SetRotation(void *instance, struct CGPoint rotation) {
    if (isAuthorized && aimbot90) {
        // Interpola√ß√£o suave: o valor 0.9 representa a for√ßa de 90%
        rotation.x *= 0.9; 
        rotation.y *= 0.9;
    }
    old_SetRotation(instance, rotation);
}

// --- FUN√á√ÉO DE RECUO (NO RECOIL 90%) ---
float (*old_GetSpread)(void *instance);
float new_GetSpread(void *instance) {
    if (isAuthorized && recoil90) {
        return old_GetSpread(instance) * 0.10f; // Reduz 90% da dispers√£o
    }
    return old_GetSpread(instance);
}

// --- INTERFACE DO MENU (ROXO E PRETO) ---
@interface FlixMenu : UIView <UITextFieldDelegate>
@property (nonatomic, strong) UITextField *keyInput;
@property (nonatomic, strong) UIButton *btnAtivar;
@end

@implementation FlixMenu

- (instancetype)initWithFrame:(CGRect)frame {
    self = [super initWithFrame:frame];
    if (self) {
        self.backgroundColor = BLACK_COLOR;
        self.layer.borderColor = PURPLE_COLOR.CGColor;
        self.layer.borderWidth = 2.0f;
        self.layer.cornerRadius = 12.0f;

        UILabel *title = [[UILabel alloc] initWithFrame:CGRectMake(0, 10, frame.size.width, 30)];
        title.text = @"üü£ FLIX_FF ULTIMATE üü£";
        title.textColor = PURPLE_COLOR;
        title.textAlignment = NSTextAlignmentCenter;
        title.font = [UIFont boldSystemFontOfSize:16];
        [self addSubview:title];

        self.keyInput = [[UITextField alloc] initWithFrame:CGRectMake(20, 60, frame.size.width - 40, 40)];
        self.keyInput.placeholder = @"DIGITE A KEY...";
        self.keyInput.backgroundColor = [UIColor whiteColor];
        self.keyInput.layer.cornerRadius = 5;
        self.keyInput.textAlignment = NSTextAlignmentCenter;
        [self addSubview:self.keyInput];

        self.btnAtivar = [UIButton buttonWithType:UIButtonTypeCustom];
        self.btnAtivar.frame = CGRectMake(20, 120, frame.size.width - 40, 45);
        self.btnAtivar.backgroundColor = PURPLE_COLOR;
        [self.btnAtivar setTitle:@"ATIVAR SISTEMA" forState:UIControlStateNormal];
        self.btnAtivar.layer.cornerRadius = 8;
        [self.btnAtivar addTarget:self action:@selector(validarKey) forControlEvents:UIControlEventTouchUpInside];
        [self addSubview:self.btnAtivar];
    }
    return self;
}

- (void)validarKey {
    if ([self.keyInput.text isEqualToString:@"PG"]) {
        isAuthorized = true;
        aimbot90 = true;
        recoil90 = true;
        [self.btnAtivar setTitle:@"‚úÖ ATIVADO" forState:UIControlStateNormal];
        self.btnAtivar.backgroundColor = [UIColor greenColor];
        [self performSelector:@selector(removeFromSuperview) withObject:nil afterDelay:2.0];
    } else {
        // Alerta caso a Key esteja errada ou vazia
        UIAlertController *alert = [UIAlertController alertControllerWithTitle:@"ERRO" message:@"adicione a Key antes de prosseguir." preferredStyle:UIAlertControllerStyleAlert];
        [alert addAction:[UIAlertAction actionWithTitle:@"OK" style:UIAlertActionStyleDefault handler:nil]];
        [[UIApplication sharedApplication].keyWindow.rootViewController presentViewController:alert animated:YES completion:nil];
    }
}
@end

// --- INICIALIZADOR DA DYLIB ---
_attribute_((constructor))
static void initializeFlix() {
    dispatch_after(dispatch_time(DISPATCH_TIME_NOW, 5 * NSEC_PER_SEC), dispatch_get_main_queue(), ^{
        UIWindow *window = [UIApplication sharedApplication].keyWindow;
        FlixMenu *menu = [[FlixMenu alloc] initWithFrame:CGRectMake(window.frame.size.width/2 - 125, 100, 250, 200)];
        [window addSubview:menu];
        
        // Hooks de mem√≥ria (Offsets devem ser atualizados conforme a vers√£o)
        // MSHookFunction((void*)0x100XXXXXX, (void*)new_GetSpread, (void**)&old_GetSpread);
        // MSHookFunction((void*)0x100XXXXXX, (void*)new_SetRotation, (void**)&old_SetRotation);
    });
}
